    <ItemGroup>
        <ProjectReference Include="..\Shared.Messages\Shared.Messages.csproj" />
        <ProjectReference Include="..\Shared\Shared.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Carter" Version="8.2.1" />
