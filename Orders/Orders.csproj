        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup>
      <Compile Remove="Messages\**" />
      <Content Remove="Messages\**" />
      <EmbeddedResource Remove="Messages\**" />
      <None Remove="Messages\**" />
